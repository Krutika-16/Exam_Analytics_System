 <h2>Student Profile</h2>
            <% studentdetails.forEach(function(item) {%>
            <div class="card-body">
              <div class="d-flex justify-content-between align-items-center">
                <form class="form-inline" action="/searchstudent" method="POST">
                  <input class="form-control mr-sm-2" name="name1" value="<%=item.name%>" type="text" placeholder="Search" aria-label="Search">
                  <button class="btn btn-info btn-sm" type="submit">Search</button>
                </form>
              </div>
            </div>
            <% }) %>  